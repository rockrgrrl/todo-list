<h3><%= @list.name %></h3>

<ul>
  <% @list.tasks.each do |task| %>
    <li> <%= task.description %>
         <%= link_to "Edit", edit_list_task_path(task.list, task) %>
         <%= link_to "Delete", list_task_path(task.list, task), method: :delete %>
    </li>
  <% end %>
</ul>

<%= link_to "Back", lists_path %>
<%= link_to "Add Task", new_list_task_path(@list)%>