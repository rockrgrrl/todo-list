<h1>Your ToDo Lists</h1>
<ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %>
        <%= link_to "Edit", edit_list_path(list) %>
        <%= link_to "Delete", list_path(list), method: :delete %>
    </li>
  <% end %>
</ul>
<p>
  <%= link_to "Make New List", new_list_path %>
</p>